
config setup
        charondebug     =   "all"
        uniqueids       =   yes
        

conn gotoadmins-to-client
        authby          =       secret
        left            =       %defaultroute
        leftid          =       
           leftsubnet      =       
             right           =       
             rightsubnet     =       
              ike             =       aes256-sha2_256-modp1024!
        esp             =       aes256-sha2_256!
        keyingtries     =       0
        ikelifetime     =       8h
        lifetime        =       1h
        dpddelay        =       30
        dpdtimeout      =       120
        dpdaction       =       restart
        auto            =       start

